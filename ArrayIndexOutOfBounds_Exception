public class ArrayIndexOutOfBoundException {  
  
    public static void main(String[] args) {  
String[] arr = {"Rohit","Shikar","Virat","Dhoni"};   
                                       
          
        for(int i=0;i<=arr.length;i++) {       
  

             System.out.println(arr[i]);      
          
        }  
  
    }  
  
}  
